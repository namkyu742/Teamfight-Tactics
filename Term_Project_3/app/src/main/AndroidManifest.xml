<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.ac.kumoh.s20161138.term_project_3">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@drawable/icon0"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Term_Project_3">

        <activity android:name=".desc_BasicItem_Activity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".desc_BasicItem_Activity"/>
        </activity>
        <activity android:name=".desc_CombiItem_Activity">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value=".desc_CombiItem_Activity"/>
        </activity>


        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>